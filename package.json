    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-sse": "^0.5.3",
    "node-fetch": "^3.3.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "bun": "^1.0.30"
  },
  "publishConfig": {
    "access": "public"
